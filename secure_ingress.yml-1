apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: secure-ingress
spec:
  tls:
  - hosts:
     - secure-ingress.cluster2.avi.com
    secretName: cert01
  rules:
    - host: secure-ingress.cluster2.avi.com
      http:
        paths:
          - pathType: Prefix
            path: "/"
            backend:
              service:
                name: web-front-1
                port:
                  number: 80
